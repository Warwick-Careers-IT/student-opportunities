    <div class="row scs-content-pad-top-30">
          <div class="col-sm-1"> </div>
          <div class="col-sm-7 ">

            <div class="topic-header">
              {# <h3>All Warwick </h3> #}
              <h1 class="topic-title"> {% block topic_title %}{% endblock %}</h1>
            </div>

          </div>
    </div>

        <div class="row scs-content-pad-top-20">

            <section class="col-sm-1">
              <h5 class="scs-topic-atoz">AtoZ</h5>
            </section>

            <section class="col-sm-7 ">

            <div class="scs-section-pane">
              <div class="scs-pane-inner" >
